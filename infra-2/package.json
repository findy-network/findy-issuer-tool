{
  "name": "infra-2",
  "version": "0.1.0",
  "bin": {
    "infra-2": "bin/infra-2.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "pipeline:init": "aws ssm put-parameter --name \"findy-issuer-tool-github-connection-arn\" --value \"$GITHUB_CONNECTION_ARN\" --type String",
    "pipeline:context": "JSON=$(tr -d '\n' < cdk.context.json) && aws ssm put-parameter --overwrite --name \"findy-issuer-tool-cdk-context\" --value \"$JSON\" --type String"
  },
  "devDependencies": {
    "aws-cdk": "2.20.0",
    "@types/jest": "^26.0.10",
    "@types/node": "10.17.27",
    "jest": "^26.4.2",
    "ts-jest": "^26.2.0",
    "ts-node": "^9.0.0",
    "typescript": "~3.9.7"
  },
  "dependencies": {
    "aws-cdk-lib": "2.20.0",
    "constructs": "^10.0.0"
  }
}
