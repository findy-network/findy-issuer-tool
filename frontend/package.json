{
  "name": "findy-issuer-app",
  "version": "0.1.46",
  "description": "Sample service app for issuing credentials via Findy Network.",
  "main": "src/index.js",
  "scripts": {
    "build": "rimraf build/* && export NODE_ENV=production && webpack --config ./webpack/prod.js && npm run build:version",
    "build:version": "echo v$(./tools/version.sh) > build/version.txt",
    "release": "npm version minor && git push --tags",
    "start": "webpack serve --port 8081 --open \"Google Chrome\" --config ./webpack/dev.js",
    "start:e2e": "webpack serve --port 8081 --config ./webpack/dev.js",
    "test": "npm run test:lint && npm run test:unit",
    "test:lint": "eslint ./src",
    "test:lint:fix": "eslint ./src --fix",
    "test:unit": "jest ./src --coverage",
    "test:update": "jest ./src --updateSnapshot",
    "update:deps": "npm-outdated-to-latest"
  },
  "repository": {
    "type": "git",
    "url": "TODO"
  },
  "keywords": [
    "SSI",
    "Findy",
    "DLT",
    "hyperledger-indy"
  ],
  "author": "TODO",
  "license": "Apache-2.0",
  "jest": {
    "setupFiles": [
      "jest-localstorage-mock"
    ],
    "testRegex": ".*\\.test\\.js$",
    "testEnvironment": "node",
    "transform": {
      ".+\\.jsx?$": "babel-jest"
    }
  },
  "dependencies": {
    "@babel/runtime": "^7.17.9",
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "connected-react-router": "^6.9.1",
    "history": "^4.10.1",
    "immutable": "^4.0.0",
    "prop-types": "15.7.2",
    "qrcode.react": "^1.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.5",
    "react-router": "^5.2.1",
    "react-router-dom": "^5.3.0",
    "redux": "^4.1.2",
    "redux-devtools-extension": "^2.13.9",
    "redux-observable": "^2.0.0",
    "rxjs": "^7.4.0",
    "seamless-immutable": "7.1.4",
    "styled-components": "^5.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.15.6",
    "@babel/plugin-transform-runtime": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-react": "^7.16.7",
    "babel-core": "7.0.0-bridge.0",
    "babel-loader": "^8.2.4",
    "babel-plugin-styled-components": "^2.0.6",
    "config": "^3.3.6",
    "eslint": "^8.12.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.3.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^27.4.7",
    "jest-localstorage-mock": "^2.4.18",
    "npm-outdated-to-latest": "^1.0.3",
    "prettier": "^2.5.1",
    "react-test-renderer": "^17.0.2",
    "rimraf": "3.0.2",
    "source-map-loader": "^3.0.0",
    "svg-url-loader": "^7.1.1",
    "url-loader": "^4.1.1",
    "webpack": "^5.59.1",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.3.1",
    "webpack-merge": "^5.8.0"
  }
}
