{
  "name": "infra",
  "version": "0.2.11",
  "bin": {
    "infra": "bin/infra.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "pipeline:init": "./tools/init.sh",
    "pipeline:context": "JSON=$(cat cdk.context.json) && aws ssm put-parameter --overwrite --name \"/findy-issuer-tool/cdk-context\" --value \"$JSON\" --type String"
  },
  "devDependencies": {
    "aws-cdk": "2.43.0",
    "@types/jest": "^26.0.10",
    "@types/node": "18.7.14",
    "jest": "^26.4.2",
    "ts-jest": "^26.2.0",
    "ts-node": "^10.9.1",
    "typescript": "~4.8.2"
  },
  "dependencies": {
    "aws-cdk-lib": "2.43.0",
    "constructs": "^10.1.93"
  }
}
